{
  "manifest_version" : 2,
  "name": "FB Quick Message",
  "version": "0.1",
  "content_scripts": [
    {
      "matches":[
        "https://www.facebook.com/*"
      ],
      "js": [
        "contentScripts/jquery-3.3.1.min.js",
        "contentScripts/fb.js",
        "contentScripts/content.js"
      ]
    }
  ],
  "browser_action": {
      "default_icon" : "resources/_Graphic/icon.png"
  }
}
